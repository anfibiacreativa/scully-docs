{
  "$schema": "../node_modules/@angular-devkit/schematics/collection-schema.json",
  "schematics": {
    "add-doc": {
      "description": "Adds a new document file",
      "factory": "./add-doc/index#addDoc",
      "aliases": ["doc"],
      "schema": "./add-doc/schema.json"
    },
    "docs-module": {
      "description": "Adds a docs module to any angular app",
      "factory": "./docs-module/index#docsModule",
      "aliases": ["docs"]
    },
    "add-docs-module-files": {
      "description": "Adds the necessary files for the docs modules",
      "factory": "./add-docs-module-files/index#addDocsModuleFiles"
    },
    "add-side-nav": {
      "description": "Adds the side navigation",
      "factory": "./add-side-nav/index#addSideNav"
    },
    "add-sidenav-files": {
      "description": "Add the necessary files for the side navigation component",
      "factory": "./add-sidenav-files/index#addSidenavFiles"
    },
    "update-scully-config": {
      "description": "Updates the scully config",
      "factory": "./update-scully-config/index#updateScullyConfig"
    },
    "ng-add": {
      "description": "Schematic to add and install the package",
      "factory": "./ng-add/index"
    }
  }
}
